{
    "name": "dev-api-rest-ts",
    "version": "1.0.0",
    "description": "Desafio API Rest com Typescript",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "lint": "eslint --ext .ts ./src",
        "format": "prettier --write \"**/*.{ts,md,json}\" --ignore-path .prettierignore",
        "start": "npm run build && node dist/index.js",
        "build": "rimraf dist && tsup src/index.ts",
        "dev": "tsx watch src/index.ts",
        "knex:rollback": "knex migrate:rollback --all --knexfile ./src/knexfile.ts",
        "knex:latest": "knex migrate:latest --knexfile ./src/knexfile.ts",
        "knex:down": "knex migrate:down --knexfile ./src/knexfile.ts",
        "knex:up": "knex migrate:up --knexfile ./src/knexfile.ts",
        "knex:seed": "knex seed:run --knexfile ./src/knexfile.ts"
    },
    "author": "",
    "license": "ISC",
    "keywords": [],
    "dependencies": {
        "cors": "^2.8.5",
        "dayjs": "^1.11.7",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-rate-limit": "^6.7.0",
        "helmet": "^7.0.0",
        "knex": "^2.4.2",
        "pg": "^8.10.0",
        "pino": "^8.14.1",
        "pino-pretty": "^10.0.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "@ngneat/falso": "^6.4.0",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/node": "^20.1.4",
        "@types/pg": "^8.6.6",
        "@typescript-eslint/eslint-plugin": "^5.59.5",
        "eslint": "^8.40.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-config-standard-with-typescript": "^34.0.1",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-n": "^15.7.0",
        "eslint-plugin-promise": "^6.1.1",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.0",
        "sqlite3": "^5.1.6",
        "tsup": "^6.7.0",
        "tsx": "^3.12.7",
        "typescript": "^5.0.4"
    }
}
